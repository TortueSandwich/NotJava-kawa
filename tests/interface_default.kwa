var paire p;



interface n_uplet {
  default method int somme(){return this.x + this.y;}
}

class paire implements n_uplet{
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  method int produit() {
    return this.x * this.y;
  }
}

main {
  p = new paire(3, 4);
  print(p.produit());
  print(p.somme());
  //affiche 12 7
}