var point p;

class point {
  attribute int x;
  attribute int y;

  method void constructor(int a, int b) {
    var int c = 10;
    this.x = a;
    this.y = b;
    print(c);
  }

  method int test() {
    var int a = this.x+this.y;
    return a*a;
  }
}

main {
  p = new point(4,5);
  print(p.test());
  // affiche 81
}
