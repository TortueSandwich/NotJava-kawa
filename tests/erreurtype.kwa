var triple t;
var triple t2;

class paire {
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  method int somme() {
    return this.x + this.y;
  }
}

class triple extends paire {
  attribute int z;

  method void constructor(int x, int y, int z) {
    this.x = x; this.y = y; this.z = z;
  }

  method int somme() {
    return this.x + this.y + this.z;
  }
}

main {
  t = new paire(3, 4);
}